{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "sourceMap": true,
    "target": "es5",
    "experimentalDecorators": true,
    "module": "esnext",
    "downlevelIteration": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "baseUrl": ".",
    "paths": {
      "Library/*": ["./Library/*"],
      "Feature/*": ["./Features/*"],
      "Image/*": ["./Image/*"],
      "Element/*": ["./Elements/*"],
    },
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.promise"
    ],
    "plugins": [
      {
        "name": "typescript-tslint-plugin"
      }
    ]
  },
  // "files": [
  //   "Features/all-pages.ts"
  // ],
  "compileOnSave": false,
  "exclude": [
    "obj",
    "node_modules"
  ]
}